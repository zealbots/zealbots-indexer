{
    "manifest_version": 2,
    "description": "Zealbots-Indexer",
    "version": "1.1,",
    "name":"zealbots",
    
    "background": {
        "scripts": ["bg.js"]
      },
    "content_scripts":[
      {
        "matches" : ["<all_urls>"],
        "js":["cs.js"]
      }
    ],
    "page_action":{
        "default_icon": "logo.png",
        "default_popup": "index.html"
        
    },

    "permissions": [
        "activeTab",
        "tabs"
      ]
}